-- An example of initiating a matrix and setting values in a loop 
NN := 5
mymat : Matrix (Complex(DoubleFloat)) := new (NN ,NN ,0)
mymat2 : Matrix (Complex(DoubleFloat)) := new (NN ,NN ,0)

for m in 1..NN repeat 
    for n in 1..NN repeat
     mymat(m,n) := m*n
     mymat2(m,n) := m+n



